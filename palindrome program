###Checking pallindrome or NotImplemented

def pallindrome(str1):
    str1=str1.upper()
    j=len(str1)-1
    i=0
    flag=0
    while(i<j):
        if(str1[i]!=str1[j]):
            flag=1 
            break
        i=i+1 
        j=j-1
    if(flag==0):
        print("pallindrome")
    else:
        print("not pallindrome")
        

str2=input("Enter string: ")
pallindrome(str2)
